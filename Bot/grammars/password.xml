<grammar version='1.0' xml:lang='en' root='root' xmlns='http://www.w3.org/2001/06/grammar'>
	<rule id='root'>
		<one-of>
			<item><ruleref uri='bomb.xml'/></item>
			<item>
				<one-of>
					<item> new </item>
					<item> next </item>
					<item> reset </item>
				</one-of>
				<one-of>
					<item> password </item>
					<item> set </item>
				</one-of>
			</item>
			<item>
				<item repeat='6'>
					<ruleref uri='bomb.xml#letter'/>
				</item>
			</item>
		</one-of>
	</rule>
</grammar>
