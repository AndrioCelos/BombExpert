<grammar version='1.0' xml:lang='en-gb' root='root' xmlns='http://www.w3.org/2001/06/grammar'>
	<rule id='root'>
		<one-of>
			<item><ruleref uri='bomb.xml'/></item>
			<item>
				<ruleref uri='bomb.xml#module'/>
				<item repeat='0-1'><ruleref uri='bomb.xml#number'/></item>
				<item repeat='0-1'>
					<one-of>
						<item><ruleref uri='bomb.xml#number'/></item>
						<item> first </item>
						<item> second </item>
						<item> third </item>
						<item> fourth </item>
						<item> fifth </item>
						<item> sixth </item>
						<item> seventh </item>
						<item> eighth </item>
						<item> ninth </item>
						<item> tenth </item>
					</one-of>
				</item>
			</item>
		</one-of>
	</rule>
</grammar>
