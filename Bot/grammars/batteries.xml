<grammar version='1.0' xml:lang='en' root='root' xmlns='http://www.w3.org/2001/06/grammar'>
	<rule id='root'>
		<one-of>
			<item><ruleref uri='bomb.xml'/></item>
			<item> none </item>
			<item> no batteries </item>
			<item>
				<ruleref uri='bomb.xml#number'/>
				<one-of>
					<item> batteries </item>
					<item> battery </item>
				</one-of>
				<item repeat='0-1'>
					<item repeat='0-1'> in </item>
					<ruleref uri='bomb.xml#number'/>
					<one-of>
						<item> holders </item>
						<item> holder </item>
					</one-of>
				</item>
			</item>
		</one-of>
	</rule>
</grammar>
