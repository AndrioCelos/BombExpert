<grammar version='1.0' xml:lang='en-gb' root='root' xmlns='http://www.w3.org/2001/06/grammar'>
	<rule id='root'>
		<one-of>
			<item><ruleref uri='bomb.xml'/></item>
			<item>
				<one-of>
					<item> new </item>
					<item> next </item>
					<item> reset </item>
				</one-of>
				stage
				<item repeat='0-1'><ruleref uri='#label'/></item>
			</item>
			<item><ruleref uri='#label'/></item>
			<item> got it </item>
			<item> back </item>
			<item> go back </item>
		</one-of>
	</rule>
	<rule id='label' scope='public'>
		<item repeat='0-1'>
			<one-of>
				<item> letter </item>
				<item> letters </item>
				<item> word </item>
				<item> words </item>
			</one-of>
		</item>
		<one-of>
			<item repeat='1-8'>
				<one-of>
					<item><ruleref uri='bomb.xml#letter'/></item>
					<item> space </item>
					<item> apostrophe </item>
					<item> double </item>
					<item> triple </item>
				</one-of>
			</item>

			<item> literally blank </item>
			<item> literal blank </item>
			<item> literally nothing </item>
			<item> literal nothing </item>
			<item> empty </item>
			<item> literally empty </item>
			<item> nil </item>

			<item> blank <item repeat='0-1'> word </item></item>

			<item> c </item>
			<item> c letter </item>
			<item> charlie </item>
			<item> charlie letter </item>

			<item> Cee Spain </item>

			<item> display </item>

			<item> done </item>

			<item> first </item>

			<item> hold </item>

			<item> hold on </item>

			<item> lead </item>
			<item> lead A </item>
			<item> lead guitar </item>

			<item> led </item>
			<item> led no A </item>

			<item> leed language </item>
			<item> Leeds England </item>
			<item> leed two Es </item>
			<item> leed two E </item>

			<item> left </item>

			<item> like </item>

			<item> middle </item>

			<item> next </item>

			<item> no </item>

			<item> nothing <item repeat='0-1'> word </item></item>

			<item> okay </item>

			<item> press </item>

			<item> read </item>
			<item> read a book</item>
			<item> read A </item>

			<item> ready </item>

			<item> red </item>
			<item> red colour </item>
			<item> colour red </item>

			<item> reed instrument </item>

			<item> right </item>

			<item> says </item>

			<item> see </item>
			<item> see a movie </item>

			<item> sure </item>

			<item> their possessive </item>
			<item> their I </item>
			<item> their with I </item>

			<item> there </item>
			<item> there location </item>
			<item> there two Es </item>
			<item> there two E </item>

			<item> they are </item>
			<item> they are words </item>
			<item> they are two words </item>
			<item> they space are </item>

			<item> they're <item repeat='0-1'> with </item> apostrophe </item>
			<item> they're contraction </item>
			<item> they apostrophe are </item>

			<item> letter U </item>
			<item> U letter </item>

			<item> uh huh <item repeat='0-1'> positive </item> </item>

			<item> uh uh <item repeat='0-1'> negative </item> </item>
			<item> staccato </item>

			<item> uhhh </item>

			<item> letters u r </item>
			<item> u r two letters </item>
			<item> u r letters </item>

			<item> wait </item>

			<item> what </item>
			<item> what statement </item>
			<item> what no question <item repeat='0-1'> mark </item> </item>

			<item> what question <item repeat='0-1'> mark </item> </item>

			<item> yes </item>

			<item> you </item>
			<item> you word </item>
			<item> word you </item>

			<item> you are </item>
			<item> words you are </item>
			<item> you are two words </item>
			<item> you are words </item>
			<item> you space are </item>

			<item> y o u apostrophe r e </item>
			<item> you apostrophe are </item>
			<item> you're <item repeat='0-1'> with </item> apostrophe </item>
			<item> you're contraction </item>

			<item> your </item>
			<item> your possessive </item>
			<item> your word </item>
			<item> your one word </item>
			<item> your no apostrophe </item>
			<item> word your </item>
		</one-of>
	</rule>
	<rule id='letter'>
		<one-of>
			<item> a </item>
			<item> b </item>
			<item> c </item>
			<item> d </item>
			<item> e </item>
