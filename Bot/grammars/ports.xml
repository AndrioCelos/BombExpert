<grammar version='1.0' xml:lang='en' root='root' xmlns='http://www.w3.org/2001/06/grammar'>
	<rule id='root'>
		<one-of>
			<item><ruleref uri='bomb.xml'/></item>
			<item>
				<item repeat='0-1'> there are </item>
				<one-of>
					<item> none </item>
					<item> no ports </item>
				</one-of>
			</item>
			<item> done </item>
			<item repeat='1-'>
				<one-of>
					<item> empty plate </item>
					<item>
						plate
						<one-of>
							<item repeat='1-'>
								<one-of>
									<item> serial </item>
									<item> parallel </item>
								</one-of>
							</item>
							<item repeat='1-'>
								<one-of>
									<item> DVI </item>
									<item> DVI D </item>
									<item> DVI digital </item>
									<item> RJ 45 </item>
									<item> RJ </item>
									<item> PS 2 </item>
									<item> PS </item>
									<item> Stereo RCA </item>
									<item> Stereo </item>
									<item> RCA </item>
								</one-of>
							</item>
						</one-of>
					</item>
				</one-of>
			</item>
		</one-of>
	</rule>
</grammar>
