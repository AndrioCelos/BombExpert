<grammar version='1.0' xml:lang='en-gb' root='root' xmlns='http://www.w3.org/2001/06/grammar'>
	<rule id='root'>
		<one-of>
			<item><ruleref uri='bomb.xml'/></item>
			<item><ruleref uri='#command'/></item>
		</one-of>
	</rule>
	<rule id='command' scope='public'>
		<item repeat='3'>
			<one-of>
				<item> natural </item>
				<item> flat </item>
				<item> sharp </item>
				<item> mordent </item>
				<item> turn </item>
				<item> common </item>
				<item> common time </item>
				<item> cut common </item>
				<item> cut common time </item>
				<item> cut time </item>
				<item> fermata </item>
				<item> clef </item>
				<item> alto </item>
				<item> alto clef </item>
				<item> C clef </item>
			</one-of>
		</item>
	</rule>
</grammar>
