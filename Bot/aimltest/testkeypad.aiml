<?xml version='1.0' encoding='UTF-8'?>
<aiml version='2.1'>
	<category>
		<pattern>XTest KeypadTests 1</pattern>
		<template>
			<think>
				<set name='SraixUnavailable'>true</set>
				<set name='topic'>Keypad</set>
				<test name='Keypad Solve Keypad 1' expected='Balloon UpsideDownY SquidKnife LeftC'>Solver Keypad Keypad Balloon UpsideDownY SquidKnife LeftC</test>
				<test name='Keypad Solve Keypad 2' expected='Euro Balloon LeftC Cursive'>Solver Keypad Keypad Euro Balloon LeftC Cursive</test>
				<test name='Keypad Solve Keypad 3' expected='Cursive DoubleK UpsideDownY HollowStar'>Solver Keypad Keypad HollowStar UpsideDownY DoubleK Cursive</test>
				<test name='Keypad Solve Keypad 4' expected='Six Paragraph DoubleK SmileyFace'>Solver Keypad Keypad Paragraph Six DoubleK SmileyFace</test>
				<test name='Keypad Solve Keypad 5' expected='Pitchfork SmileyFace BT Paragraph'>Solver Keypad Keypad Pitchfork Paragraph BT SmileyFace</test>
				<test name='Keypad Solve Keypad 6' expected='Six Euro Pitchfork Omega'>Solver Keypad Keypad Six Euro Pitchfork Omega</test>
				<test name='Keypad Solve Keypad 7' expected='NoColumn'>Solver Keypad Keypad Tracks Ae NWithHat FilledStar</test>

				<test name='Keypad Solve CheckKeypad 1' expected='true'>Solver Keypad CheckKeypad Balloon UpsideDownY SquidKnife LeftC</test>
				<test name='Keypad Solve CheckKeypad 2' expected='false'>Solver Keypad CheckKeypad Tracks Ae NWithHat FilledStar</test>
				<test name='Keypad Solve CheckKeypad 3' expected='false'>Solver Keypad CheckKeypad Tripod</test>
				<srai>Solver Keypad Start</srai>

				<srai>Solver Keypad GlyphPresent true DoubleK</srai>
				<test name='Keypad Solve Stage 2-1' expected='Tell Copyright Pumpkin Cursive DoubleK MeltedThree UpsideDownY HollowStar'>Solver Keypad GlyphPresent true HollowStar</test>

				<srai>new keypad</srai>
				<srai>Solver Keypad Start</srai>
				<test name='Keypad Solve Stage 2-2' expected='Tell Pitchfork SmileyFace BT RightC Paragraph Dragon FilledStar'>Solver Keypad GlyphPresent true RightC</test>
			</think>
		</template>
	</category>

	<category>
		<pattern>XTest Keypad Conversation 1</pattern>
		<template>
			<think>
				<srai>new bomb</srai>
				<srai>defuse keypad</srai>
			</think>
			<test name='Keypad Conversation 1' expected='OK. Describe the second glyph.'>A backward C with a dot</test>
		</template>
	</category>
	<category>
		<pattern>XTest Keypad Conversation 2</pattern>
		<template><test name='Keypad Conversation 2' expected='OK. Describe the third glyph.'>A backward E with dots</test></template>
	</category>
	<category>
		<pattern>XTest Keypad Conversation 3</pattern>
		<template><test name='Keypad Conversation 3' expected='OK. Describe the last glyph.'>A loop the loop</test></template>
	</category>
	<category>
		<pattern>XTest Keypad Conversation 4</pattern>
		<template><test name='Keypad Conversation 4' expected='&lt;oob&gt;SetGrammar keypadOK&lt;/oob&gt; Press the backward E. Then say OK.'>An upside-down question mark</test></template>
	</category>
	<category>
		<pattern>XTest Keypad Conversation 5</pattern>
		<template><test name='Keypad Conversation 5' expected='Press the backward C. Then say OK.'>OK</test></template>
	</category>
	<category>
		<pattern>XTest Keypad Conversation 6</pattern>
		<template><test name='Keypad Conversation 6' expected='Press the cursive loop. Then say OK.'>OK</test></template>
	</category>
	<category>
		<pattern>XTest Keypad Conversation 7</pattern>
		<template><test name='Keypad Conversation 7' expected='Press the upside-down question mark. Then say OK.'>OK</test></template>
	</category>
	<category>
		<pattern>XTest Keypad Conversation 8</pattern>
		<template><test name='Keypad Conversation 8' expected='The keypad should be finished now.'>OK</test></template>
	</category>
</aiml>
