<?xml version='1.0' encoding='UTF-8'?>
<aiml version='2.1'>
	<category>
		<pattern>XTest Passwords 1</pattern>
		<template>
			<think>
				<srai>defuse passwords</srai>
				<test name='Password 1-1' expected='Tell me the six possible letters for the second position.'>alfa charlie india mike quebec zulu</test>
				<test name='Password 1-2' expected="The password is 'after'.">echo foxtrot india hotel papa romeo</test>
				<srai>new password</srai>
				<test name='Password 2-1' expected="The password is 'right'.">delta mike romeo juliet victor yankee</test>
				<srai>new password</srai>
				<test name='Password 3-1' expected='Tell me the six possible letters for the second position.'>kilo tango alfa november echo x-ray</test>
				<test name='Password 3-2' expected="No passwords match. Please start over.">mike x-ray india zulu oscar romeo</test>

				<test name='Password 4-1' expected='Tell me the six possible letters for the second position.'>foxtrot sierra zulu tango papa golf</test>
				<test name='Password 4-2' expected='Tell me the six possible letters for the third position.'>hotel alfa golf whiskey tango foxtrot</test>
				<test name='Password 4-3' expected="The password is 'thing' or 'think'.">papa alfa sierra hotel whiskey delta</test>
			</think>
		</template>
	</category>
</aiml>
