<grammar version='1.0' xml:lang='en-AU' root='keypadOKRoot' xmlns='http://www.w3.org/2001/06/grammar'>
	<rule id='keypadOKRoot'>
		<one-of>
			<item><ruleref uri='bomb.xml'/></item>
			<item>
				<one-of>
					<item> new </item>
					<item> next </item>
					<item> reset </item>
				</one-of>
				<one-of>
					<item> set </item>
					<item> keypad </item>
				</one-of>
			</item>
			<item> yes </item>
			<item> no </item>
			<item> OK </item>
		</one-of>
	</rule>
</grammar>
