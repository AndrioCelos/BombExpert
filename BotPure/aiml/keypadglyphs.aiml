<?xml version="1.0" encoding="UTF-8"?>
<aiml>
	<!--
		Usage: GetKeypadGlyphName [user description ...]
		Tries to match the specified description from the Defuser to a glyph name.
		Glyph names are as defined in the KeypadGlyph set.
		It returns the glyph name, or 'unknown' if it doesn't recognise the description.
		It also sets the KeypadGlyphDescription[glyph name] predicate to the specified description.
	-->
	<category>
		<pattern>GetKeypadGlyphName a *</pattern>
		<template><srai>GetKeypadGlyphName <star/></srai></template>
	</category>
	<category>
		<pattern>GetKeypadGlyphName $A ^ T ^</pattern>
		<template>
			<think><set><name>KeypadGlyphDescriptionPyramid</name>A <star/> T <star index='2'/></set></think>
			Pyramid
		</template>
	</category>
	<category>
		<pattern>GetKeypadGlyphName $A ^ tail ^</pattern>
		<template>
			<think><set><name>KeypadGlyphDescriptionPyramid</name>A <star/> tail <star index='2'/></set></think>
			Pyramid
		</template>
	</category>
	<category>
		<pattern>GetKeypadGlyphName $A ^ penis ^</pattern>
		<template>
			<think><set><name>KeypadGlyphDescriptionPyramid</name>A <star/> tail <star index='2'/></set></think>
			Pyramid
		</template>
	</category>
	<category>
		<pattern>GetKeypadGlyphName the *</pattern>
		<template><srai>GetKeypadGlyphName <star/></srai></template>
	</category>
	<category>
		<pattern>GetKeypadGlyphName an *</pattern>
		<template><srai>GetKeypadGlyphName <star/></srai></template>
	</category>
	<category>
		<pattern>GetKeypadGlyphName letter *</pattern>
		<template><srai>GetKeypadGlyphName <star/></srai></template>
	</category>
	<category>
		<pattern>GetKeypadGlyphName *</pattern>
		<template>
			<set var='glyph'>GetKeypadGlyphName2<star/>
			<think><set><name>KeypadGlyphDescription<get var='glyph'/></name><star/></set></think>
		</template>
	</category>
	<category>
		<pattern>GetKeypadGlyphName</pattern>
		<template>unknown</template>
	</category>
	<category>
		<pattern>GetKeypadGlyphName <set>KeypadGlyph</set></pattern>
		<template><star/></template>
	</category>

